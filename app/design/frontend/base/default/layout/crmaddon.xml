<layout version="0.1.0">
    <qquoteadv_view_view>
        <reference name="head">
            <action method="addCss">
                <link>css/crmaddon.css</link>
            </action>
            <action method="addItem">
                <type>skin_js</type>
                <name>js/crmaddon.js</name>
            </action>
        </reference>
        <reference name="qquoteadv.view">
            <block type="crmaddon/crmmessages" name="crmaddon.messages">
                <block type="crmaddon/attachment" name="crmaddon.attachment" template="qquoteadv/crmaddon/attachment.phtml" />
                <block type="crmaddon/attachment" name="crmaddon.attachment.new" template="qquoteadv/crmaddon/attachment_new.phtml" />
            </block>
        </reference>
    </qquoteadv_view_view>
</layout>
